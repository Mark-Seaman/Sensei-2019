#!/bin/bash


export PIPENV_IGNORE_VIRTUALENVS=1
export PIPENV_VENV_IN_PROJECT=1
export u=$HOME/UNC


# Path variable
export PATH=$p:\
$p/bin:\
/bin:\
/sbin:\
/usr/local/sbin:\
/usr/local/bin:\
/opt/local/bin:\
/usr/bin:\
/usr/local/mysql/bin

# Setup Python Context
cd $p
[ "$p" != "$HOME" ] &&
[ "$PIPENV_ACTIVE" != '1' ] && 
figlet pipenv shell &&
pipenv shell 

